
public class dackel
{
//Instanzvariablen
int Alter;
int schulterhoehe;

//Instanzmethoden
public void bellen()
{
System.out.println
("WauWau!\n");

}
public void laufen()
{
System.out.println
("Der dackel läuft geradeaus.\n");

}
public int getAlter()
{
return this.Alter;
}
public void setAlter(int x)
{
 this.Alter = x;
}
public int getschulterhoehe()
{
return this.schulterhoehe;
}
public void setschulterhoehe(int x)
{
 this.schulterhoehe = x;
}
}
